#!/usr/bin/env zsh
if [[ -z $STOW_FOLDERS ]]; then
    STOW_FOLDERS="bindings,redshift,ssh,lazygit,programs,lessfilter,tmux,gh-envs,gitconfig,nvim,kitty,zsh,nnn,qtile,dunst"
fi

if [[ -z $DOTFILES ]]; then
    DOTFILES=$HOME/.dotfiles
fi

STOW_FOLDERS=$STOW_FOLDERS DOTFILES=$DOTFILES $DOTFILES/install

alias vimrc="vim ~/.vimrc" \
      dps="docker ps" \
      dils="docker image ls" \
      dipn="docker image prune" \
      dnls="docker network ls" \
      dbpn="docker builder prune" \
      dclsa="docker container ls --all" \
      r='ranger --choosedir=$HOME/.rangerdir; LASTDIR=`cat $HOME/.rangerdir`; cd "$LASTDIR"' \
      rdwm="sudo make clean install" \
      dwmc="vim ~/dwm-6.1/config.h" \
      wk="xbindkeys --key" \
      wmk="xbindkeys --multikey" \
      rxbk="xbindkeys --poll-rc" \
      xbkrc="vim ~/.xbindkeysrc" \
      gwi="xprop | awk '
      	/^WM_CLASS/{sub(/.* =/, \"instance:\"); sub(/,/, \"\nclass:\"); print}
      	/^WM_NAME/{sub(/.* =/, \"title:\"); print}'" \
      bst="acpi" \
      slsc="vim ~/slstatus/config.h" \
      rsls="sudo make clean install" \
      cc="vim ~/.config/compton.conf" \
      config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME' \
      blue="bluetoothctl" \
      efc="fc-cache -fv" \
      civpn="sudo openfortivpn -c $HOME/Documents/openfortivpn/openfortivpn-config > $HOME/Documents/openfortivpn/outputvpn.log 2>&1 &" \
      cips="ssh jjjaramilloar@172.23.177.79" \
      cids="ssh jjjaramilloar@172.23.177.238" \
      qtilec="vim ~/.config/qtile/config.py" \
      qtilecd="cd ~/.config/qtile" \
      qtilel="cat ~/.local/share/qtile/qtile.log" \
      qtilerl="rm ~/.local/share/qtile/qtile.log" \
      pl="curl parrot.live" \
      rick="curl -s -L http://bit.ly/10hA8iC | bash" \
      ild="cd $HOME/Documents/IDEA-LOCAL" \
      dbeaver="{flatpak run io.dbeaver.DBeaverCommunity > /dev/null 2>&1 &} && disown" \
      discord="{flatpak run com.discordapp.Discord > /dev/null 2>&1 &} && disown" \
      spotify="{flatpak run com.spotify.Client > /dev/null 2>&1 &} && disown" \
      zsha="vim ~/.zsh_aliases" \
      zshf="vim ~/.zsh_functions" \
      rcs="colorscript random" \
      ist="speedtest" \
      re="random_emoji" \
      cnc="chuck_cow" \
      lcm="xrandr --listmonitors" \
      crt="cool-retro-term" \
      src="omz reload" \
      lud="cd $HOME/Documents/UNIVERSITY-LOCAL/NOVENA\ MATRICULA" \
      pp="cd $HOME/Documents/PERSONAL-PROJECTS" \
      dnvpn="nordvpn disconnect" \
      raptp="sudo xargs -a packages_list.txt apt install" \
      clr="clear " \
      rhe="cd $HOME/Documents/PERSONAL-DATA-REMOTE/DATOS\ CASA\ \(PINAR\)/PAGOS\ 2021/" \
      tipsc="ping 172.23.177.79" \
      codei="eval \`ssh-agent\` && ssh-add && code ." \
      codem="eval \`ssh-agent\` && ssh-add ~/.ssh/id_ed25519-melt && code ." \
      zshrc="vim $HOME/.zshrc" \
      cnst="config status" \
      cnl="config pull" \
      cnp="config push" \
      cna="config add" \
      cncmsg="config commit -m" \
      cnlg="config log --stat" \
      cnd="config diff" \
      le="exa --header --long --git --icons --links --all" \
      bcat="batcat" \
      duf="duf" \
      fprev="fzf --preview 'batcat --color=always --style=numbers --line-range=:500 {}'" \
      gspf="ncdu --exclude './Documents/*-REMOTE'"
